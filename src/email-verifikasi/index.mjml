          <mj-social-element
            padding="0px 8px"
            href="https://twitter.com/majooindonesia"
            src="/assets/icons/Twitter.svg"
          ></mj-social-element>

          <mj-social-element
            padding="0px 8px"
            href="https://www.facebook.com/revolusimajoo"
            src="/assets/icons/Facebook.svg"
          ></mj-social-element>

          <mj-social-element
            padding="0px 8px"
            href="https://www.youtube.com/c/majooIndonesia"
            src="/assets/icons/Youtube.svg"
          ></mj-social-element> </mj-social
